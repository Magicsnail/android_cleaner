<?xml version="1.0" encoding="UTF-8"?>
<project>
    <version>1.0</version>
	<!-- 注意：所有的路径均相对于工程根目录，当前仅支持Android studio标准方式的根目录 -->
	<!-- modules: 用于支持按目录keep -->

    <modules>
    	<module path="/xapp" />
    	<module path="/libraries/yapp" />
    </modules>

    <drawables>
    	<drawable file="/xxx/src/main/res/drawable-xxhdpi/xxxxxxx.png" remark="keep指定文件，路径为相对路径"/>
        <drawable prefix="xyz_" remark="keep以xyz_为前缀的drawable" />
	</drawables>

    <layouts>
        <layout file="/biz_circles/src/main/res/layout/yyyyy.xml" remark="keep指定的layout"/>
		<layout prefix="abc_" remark="keep指定前缀的layout文件" />
    </layouts>

</project>

